# Copy this file to terraform.tfvars and fill in the values
# DO NOT commit terraform.tfvars to version control

aws_region = "us-east-1"

# VPC and networking
vpc_cidr = "10.0.0.0/16"
availability_zones = ["us-east-1a", "us-east-1b", "us-east-1c"]
private_subnets = ["10.0.1.0/24", "10.0.2.0/24", "10.0.3.0/24"]
public_subnets = ["10.0.101.0/24", "10.0.102.0/24", "10.0.103.0/24"]

# EKS
eks_version = "1.28"

# MongoDB Atlas
mongodb_project_name = "sleepr"
mongodb_cluster_type = "REPLICASET"
mongodb_instance_size = "M10"
mongodb_version = "6.0"
mongodb_region = "US_EAST_1"

# Secrets - Fill these in with your actual values
mongodb_atlas_public_key = "your_mongodb_atlas_public_key"
mongodb_atlas_private_key = "your_mongodb_atlas_private_key"
mongodb_atlas_org_id = "your_mongodb_atlas_org_id"
jwt_secret = "your_jwt_secret"
stripe_secret_key = "your_stripe_secret_key"
google_oauth_client_id = "your_google_oauth_client_id"
google_oauth_client_secret = "your_google_oauth_client_secret"
google_oauth_refresh_token = "your_google_oauth_refresh_token"